{
	"github": {
		"release": true
	},
	"git": {
		"commitMessage": "release: v${version}",
		"tagName": "wx-tool-ci-v%s"
	},
	"npm": {
		"publish": true
	},
	"hooks": {
		"after:bump": "echo 更新版本成功"
	},
	"plugins": {
		"@release-it/conventional-changelog": {
			"preset": "angular",
			"infile": "CHANGELOG.md"
		}
	}
}
